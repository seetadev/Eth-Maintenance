{
  "name": "pcr",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "chain": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json node",
    "compile": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json compile",
    "fork": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json node  --network hardhat",
    "deploy": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json run ./scripts/deploy.ts",
    "task": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json",
    "contracts:test": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json test",
    "contracts:coverage": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json coverage",
    "watch-contract": "cd hardhat && node scripts/helpers/watch.js",
    "watch-tinker": "cd hardhat && node scripts/helpers/tinker-watch.js",
    "wire": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json run ./scripts/helpers/wire.ts",
    "tinker": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json run ./scripts/tinker.ts",
    "publish:subgraph": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json run ./scripts/publish.ts",
    "codegen": "cd add-ons/subgraph && graph codegen",
    "clone": "cd hardhat && npx hardhat --tsconfig ./tsconfig.hardhat.json run ./scripts/clone.ts",
    "build-graph": "cd add-ons/subgraph && graph build",
    "deploy-graph": "cd add-ons/subgraph  && graph deploy --node https://api.thegraph.com/deploy/ donoso-eth/perpetual-conditional-reward",
    "run-graph-node": "cd add-ons/graph-node && docker-compose up",
    "remove-graph-node": "cd add-ons/graph-node && docker-compose down",
    "clean-graph-node": "rimraf add-ons/graph-node/data/",
    "clean-mac-node": "rm -rf add-ons/graph-node/data/",
    "create-graph-local": "cd add-ons/subgraph && graph create --node http://localhost:8020/ donoso-eth/perpetual-conditional-reward",
    "remove-graph-local": "cd add-ons/subgraph && graph remove --node http://localhost:8020/ donoso-eth/perpetual-conditional-reward",
    "deploy-graph-local": "cd add-ons/subgraph && graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 donoso-eth/perpetual-conditional-reward"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.4",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@apollo/client": "^3.5.10",
    "@download/blockies": "^1.0.3",
    "@ngrx/store": "^13.0.2",
    "@superfluid-finance/sdk-core": "^0.3.2",
    "apollo-angular": "^3.0.0",
    "chart.js": "^3.3.2",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.0.0",
    "graphql": "^16.3.0",
    "primeflex": "^3.1.0",
    "primeicons": "5.0.0",
    "primeng": "^13.3.3",
    "prismjs": "1.9.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
